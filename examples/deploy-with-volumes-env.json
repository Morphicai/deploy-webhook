{
  "name": "myproxy",
  "repo": "focusbe/myproxy",
  "version": "latest",
  "port": 8901,
  "containerPort": 3000,
  "volumes": [
    "/opt/myproxy/storage/my-proxy/data:/app/data",
    "/opt/myproxy/logs:/app/logs",
    "/etc/ssl/certs:/etc/ssl/certs:ro"
  ],
  "environment": [
    "CONFIG_PATH=/app/data/config.json",
    "NODE_ENV=production",
    "LOG_LEVEL=info",
    "PORT=3000",
    "ENABLE_METRICS=true"
  ],
  "secret": "$WEBHOOK_SECRET"
}