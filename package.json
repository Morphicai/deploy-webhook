{
  "name": "deploy-webhook",
  "version": "1.0.0",
  "private": true,
  "description": "Docker-based deployment webhook server for Morphix AI App Shell",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "PORT=9000 nodemon --exec ts-node src/index.ts",
    "dev:build": "tsc --watch",
    "clean": "rm -rf dist",
    "docker:build": "docker build -t deploy-webhook .",
    "docker:run": "docker run --rm -p 9000:9000 -v /var/run/docker.sock:/var/run/docker.sock deploy-webhook"
  },
  "dependencies": {
    "@infisical/sdk": "^4.0.6",
    "better-sqlite3": "^12.4.1",
    "dockerode": "^4.0.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/dockerode": "^3.3.26",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.5",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
